{
  "name": "imunew/tymon-jwt-auth",
  "authors": [
    {
      "name": "imunew",
      "email": "imunew@gmail.com"
    }
  ],
  "license": "MIT",
  "require": {
    "php": "^7.0|^8.0",
    "tymon/jwt-auth": "^1.0",
    "illuminate/support": "^5.5|^6|^7|^8"
  },
  "require-dev": {
    "orchestra/testbench": "^5.1"
  },
  "autoload": {
    "psr-4": {
      "Imunew\\JWTAuth\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Imunew\\JWTAuth\\Providers\\ServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  },
  "scripts": {
    "cs-fixer-check": [
      "PHP_CS_FIXER_IGNORE_ENV=1 ~/.composer/vendor/bin/php-cs-fixer fix --dry-run -v"
    ],
    "cs-fixer-fix": [
      "PHP_CS_FIXER_IGNORE_ENV=1 ~/.composer/vendor/bin/php-cs-fixer fix --diff -v"
    ],
    "test": [
      "phpdbg -qrr vendor/bin/phpunit"
    ]
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
